{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2017",
    "moduleResolution": "Node",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noEmit": true
  },
  "include": ["src"]
}
